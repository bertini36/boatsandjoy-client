{
  "name": "quicopou",
  "version": "1.0.0",
  "scripts": {
    "serve": "vue-cli-service serve --host 0.0.0.0 --port 3000",
    "build": "npm run build:tailwind && vue-cli-service build --modern",
    "lint": "vue-cli-service lint",
    "serve:tailwind": "postcss src/assets/css/main.css -o public/css/output.css -w",
    "build:tailwind": "NODE_ENV=production postcss src/assets/css/main.css -o public/css/output.css"
  },
  "dependencies": {
    "core-js": "^3.6.5",
    "vue": "^3.0.0",
    "vue-router": "^4.0.2",
    "vuex": "^4.0.0-rc.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.0.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^7.0.0-0",
    "tailwindcss": "^2.0.1",
    "@tailwindcss/typography": "^0.3.1",
    "autoprefixer": "^9.7.5",
    "postcss": "^7.0.35",
    "postcss-cli": "^7.1.0",
    "postcss-loader": "^4.1.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
